    "remove-local-bnb": "graph remove --node http://localhost:8020/ niftyswap/niftyswap-bnb",
    "deploy-local-bnb": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 niftyswap/niftyswap-bnb",
