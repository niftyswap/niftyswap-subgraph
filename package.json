    "deploy-local-matic-dev": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 niftyswap/dev-niftyswap-polygon",
    "generate-matic-dev": "rm subgraph.yaml && mustache ./networks/polygon.json subgraph.template.yaml subgraph.yaml",

