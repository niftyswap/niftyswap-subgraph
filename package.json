    "deploy-mumbai": "yarn generate-mumbai && graph deploy --node https://api.thegraph.com/deploy/ niftyswap/niftyswap-mumbai",
    "create-local-mumbai": "graph create --node http://localhost:8020/ niftyswap/niftyswap-mumbai",
    "remove-local-mumbai": "graph remove --node http://localhost:8020/ niftyswap/niftyswap-mumbai",
