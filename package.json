{
  "name": "niftyswap",
  "license": "UNLICENSED",
  "version": "1.0.2",
  "scripts": {
    "print-version": "echo $npm_package_version",
    "update-version": "npm version patch",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-dev": "yarn deploy-matic-dev",
    "deploy-all": "yarn update-version && yarn deploy-mainnet && yarn deploy-mainnet-hosted && yarn deploy-matic-hosted && yarn deploy-mumbai && yarn deploy-bnb && yarn deploy-arbitrum && yarn deploy-optimism && yarn deploy-avalanche && yarn deploy-gnosis && yarn deploy-goerli",
    "deploy-mainnet": "yarn generate-mainnet && graph deploy --studio niftyswap-mainnet --version-label $npm_package_version",
    "create-local-mainnet": "graph create --node http://localhost:8020/ niftyswap/niftyswap-mainnet",
    "remove-local-mainnet": "graph remove --node http://localhost:8020/ niftyswap/niftyswap-mainnet",
    "deploy-local-mainnet": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 niftyswap/niftyswap-mainnet",
    "generate-mainnet": "rm subgraph.yaml && mustache ./networks/mainnet.json subgraph.template.yaml subgraph.yaml",
    
    "deploy-mainnet-hosted": "yarn generate-mainnet-hosted && graph deploy --node https://api.thegraph.com/deploy/ niftyswap/niftyswap-mainnet",
    "create-local-mainnet-hosted": "graph create --node http://localhost:8020/ niftyswap/niftyswap-mainnet",
    "remove-local-mainnet-hosted": "graph remove --node http://localhost:8020/ niftyswap/niftyswap-mainnet",
    "deploy-local-mainnet-hosted": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 niftyswap/niftyswap-mainnet",
